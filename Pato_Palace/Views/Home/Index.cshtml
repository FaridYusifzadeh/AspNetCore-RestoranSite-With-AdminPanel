@model HomeModel
@{
    ViewData["Title"] = "Home";
}

<main>

    <!-- Slider -->

    <div class="swiper-container">
        <div class="swiper-wrapper">

            @foreach (var slider in Model.Sliders)
            {
                <div class="swiper-slide">

                    <div class="slider_text" style="background-image: url(Images/@slider.Image)">
                        <p class="text-center"> @slider.Description</p>
                        <h2 class="text-center">@slider.Title</h2>
                        <h6 class="text-center">@slider.SubTitle </h6>
                    </div>

                </div>

            }




        </div>

        <!-- Add Arrows -->
        <div class="swiper-button-next"></div>
        <div class="swiper-button-prev"></div>
    </div>



    <!-- top scroll -->
    <a href="#" class="back-to-top"> <i class="fas fa-angle-double-up"></i> </a>
    <!-- top scroll -->
    <!-- header ends -->
    <!-- unique section starts -->
    <section class="unique">
        <div class="container">
            <div class="row">
                <div class="col-12 firsth-section text-center">
                    @foreach (var word in Model.UniqueHistories)
                    {
                    <h1>

                        @word.Title
                        <br />

                        @Model.Sliders.ElementAt(0).Title.ToUpper()
                    </h1>
                       
                         

                        <p class="unique-text">

                            @word.Description
                        </p>
                        <p class="unique-name">@word.SubDescription</p>
                    }

                </div>

            </div>
        </div>
    </section>
    <!-- unique section ends -->
    <!-- section photo-menu starts -->
    <section class="photo-menu">

        <div class="container-fluid">
            <div class="row mt-5">
                <div class="col-md-8 mt-5">
                    <div class="menu-image1">
                        <div class="layout">
                            <span>@Model.Photo_Menus.ElementAt(0).Title</span>
                        </div>
                        <img src="~/images/@Model.Photo_Menus.ElementAt(0).Image" alt="">
                    </div>
                </div>
                <div class="col-md-4 d-flex justify-content-between">
                    <div class="row ">
                        <div class="col-md-12 one mt-5 ">
                            <div class="menu-image1">
                                <div class="layout" style="color:#616060">
                                    <span>@Model.Photo_Menus.ElementAt(1).Title</span>
                                </div>
                                <img src="~/images/@Model.Photo_Menus.ElementAt(1).Image" alt="">
                            </div>
                        </div>
                        <div class="col-md-12 tho mt-auto">
                            <div class="menu-image1">
                                <div class="layout">
                                    <span>@Model.Photo_Menus.ElementAt(2).Title</span>
                                </div>
                                <img src="~/images/@Model.Photo_Menus.ElementAt(2).Image" alt="">
                            </div>
                        </div>

                    </div>
                </div>

            </div>

            <div class="row mt-5">

                <div class="col-md-4 d-flex justify-content-between">
                    <div class="row ">
                        <div class="col-md-12 one mt-5 ">
                            <div class="menu-image1">
                                <div class="layout" style="color:#616060">
                                    <span>@Model.Photo_Menus.ElementAt(4).Title</span>
                                </div>
                                <img src="~/images/@Model.Photo_Menus.ElementAt(4).Image" alt="">
                            </div>
                        </div>
                        <div class="col-md-12 tho mt-auto">
                            <div class="menu-image1">
                                <div class="layout">
                                    <span>@Model.Photo_Menus.ElementAt(5).Title</span>
                                </div>
                                <img src="~/images/@Model.Photo_Menus.ElementAt(5).Image" alt="">
                            </div>
                        </div>

                    </div>
                </div>

                <div class="col-md-8 mt-5">
                    <div class="menu-image1">
                        <div class="layout">
                            <span>@Model.Photo_Menus.ElementAt(3).Title</span>
                        </div>
                        <img src="~/images/@Model.Photo_Menus.ElementAt(3).Image" alt="">
                    </div>
                </div>

            </div>
        </div>

    </section>
    <!-- section photo-menu ends -->
    <!-- section Menu starts -->
    <section class="menu">
        <div class="container">
            <div class="row" style="margin-top:100px">
                <div class="col-12 text-center">
                    <h5 style="font-family: 'Dancing Script', cursive; font-size:1.5em ; color: #8b755a;">
                       @Model.Main_Menus.ElementAt(0).Description
                    </h5>
                </div>
                <div class="col-12 text-center">
                    <h1 style="font-weight: 700;    position: relative; color: #232323; font-size: 42px; font-weight: 700; line-height: 1.2em; padding-bottom: 35px; letter-spacing: 1px; text-transform: uppercase;">
                       @Model.Main_Menus.ElementAt(0).Title
                    </h1>
                </div>
                <div class="container">
                    <div class="row mt-5">
                        @foreach (var menu in Model.Menu_Contents)
                        {
                            <div class="col-lg-4 col-md-12">
                                <div class="col-12 mt-5">
                                    <h5 class="d-inline">
                                        <a href=""> @menu.Title </a>
                                    </h5>
                                    <h5 class="d-inline float-right"> $@menu.Price</h5>
                                    <h6 style="    position: relative;color: #616060;font-size: 15px;font-family: 'Open Sans', sans-serif; margin-top:15px">
                                      @menu.Description
                                    </h6>
                                </div>
                               
                            </div>
                        }

                        
                    </div>
                </div>


            </div>


        </div>


    </section>

    <!-- section Menu ends -->
    <!-- section Enjoy starts -->
    <section class="enjoy-together" style=" background-image:url('images/@Model.Enjoys.ElementAt(0).Image');">


        
        <div class="container">
            <div class="row">
                <div class="col-12 text text-center ">
                    <h1 class=""
                        style="height:200px; color: #ffffff;margin-top:200px;font-size: 75px;font-weight: 700;letter-spacing: 1px;line-height: 1.2em;text-transform: uppercase;display: inline-block;">
                        @Model.Enjoys.ElementAt(0).Title
                    </h1>
                </div>
            </div>
        </div>


    </section>
    <!-- section Enjoy ends -->
    <!-- sECTION Reservation starts -->
    <section class="reservation">
        <div class="container">
            <div class="row" style="margin-top:100px">
                <div class="col-12 text-center">
                    <h5 style="font-family: 'Dancing Script', cursive; font-size:1.5em ; color: #8b755a;">
                       @Model.Reservations.ElementAt(0).Description
                    </h5>
                </div>
                <div class="col-12 text-center">
                    <h1 style="font-weight: 700;    position: relative; color: #232323; font-size: 42px; font-weight: 700; line-height: 1.2em; padding-bottom: 35px; letter-spacing: 1px; text-transform: uppercase;">
                        @Model.Reservations.ElementAt(0).Title
                    </h1>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group row">
                        <label for="example-date-input" class="col-2 col-form-label">
                            <i style="font-size:25px;" class="far fa-calendar"></i>
                        </label>
                        <div class="col-8">
                            <input class="form-control" type="date" value="2011-08-19" id="example-date-input">
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group row">
                        <label for="example-time-input" class="col-2 col-form-label">
                            <i style="font-size:25px;" class="far fa-clock"></i>
                        </label>
                        <div class="col-8">
                            <input class="form-control" type="time" value="13:45:00" id="example-time-input">
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group row">
                        <label for="example-number-input" class="col-2 col-form-label">
                            <i style="font-size:25px;" class="fas fa-user"></i>
                        </label>
                        <div class="col-8">
                            <input class="form-control" type="number" value="0" max="6" min="0" id="example-number-input">
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-12 text-center">
                    <button style="     background: #8b755a;color:white;font-weight:bold;font-size:2em;margin-top:20px !important;    border: 2px solid #8b755a;line-height: 24px;    text-align: center;   border-radius: 0px; font-size: 12px; letter-spacing: 2px; padding: 13px 43px; margin-top: 10px;cursor: pointer;text-transform:uppercase;">
                        find a table
                    </button>
                </div>
            </div>





    </section>





    <!-- SECTION Reservation ENDS -->
    <!-- SECTION Testiominal starts -->
    <section class="testimonial" style="background-image: url(Images/bacground_section/@Model.Rservation_Contents.ElementAt(0).Image">
        <div class="container">
            <div class="row">
                <div class="col-12 text-center">
                    <h1 style="margin-top:200px;position: relative;color: #ffffff;font-size: 20px;font-weight: 400;line-height: 1.6em;font-family: 'Conv_georgia';">
                        “@Model.Rservation_Contents.ElementAt(0).Description”
                    </h1>
                    <p class="mt-5"
                       style="color: #d1d1d1;font-size: 15px;font-style: italic;letter-spacing: 1px;margin-top: 30px;text-transform: capitalize;font-family: 'Conv_georgia';">
                       @Model.Rservation_Contents.ElementAt(0).Title
                    </p>
                </div>

            </div>
        </div>



    </section>



    <section class="information">
        <div class="container">
            <div class="row mt-5">
                <div class="col-lg-4">
                    <div class="row text-center">
                        <div class="col-12">
                            <h3 style="color: #232323;font-size: 24px;font-weight: 700;letter-spacing: 1px;text-transform: uppercase;">
                               @Model.Locations.ElementAt(0).Title
                            </h3>
                        </div>
                        <div class="col-12 mt-3">
                            <h5>
                                @Model.Locations.ElementAt(0).SubTitle
                            </h5>
                            <p class="mt-3" style="    position: relative;color: #616060;font-size: 14px;margin-bottom: 48px;font-family: 'Open Sans', sans-serif;">
                                @Model.Locations.ElementAt(0).Description
                            </p>
                        </div>
                        <div class="col-12">
                            <h5>
                                @Model.Locations.ElementAt(0).TitleCode

                            </h5>
                            <p class="" style="    position: relative;color: #616060;font-size: 14px;margin-bottom: 48px;font-family: 'Open Sans', sans-serif;">
                                @Model.Locations.ElementAt(0).SubTitleCode
                            </p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-8">
                    <div class="map">
                        <div style="width: 100%">
                            <div style="width: 100%">
                                <iframe width="100%" height="350"
                                        src="https://maps.google.com/maps?width=100%&amp;height=600&amp;hl=en&amp;q=1%20Grafton%20Street%2C%20Dublin%2C%20Ireland+(My%20Business%20Name)&amp;ie=UTF8&amp;t=&amp;z=16&amp;iwloc=B&amp;output=embed"
                                        frameborder="0" scrolling="no" marginheight="0" marginwidth="0">
                                    <a href="https://www.maps.ie/map-my-route/">Create a route on google maps</a>
                                </iframe>
                            </div>
                            <br />
                        </div>
                        <br />

                    </div>
                </div>
            </div>
        </div>


    </section>


    <!-- SECTION Testiominal ENDS -->
    <!-- SECTION Gift-Unique starts -->
    <section class="gift-unique" style="background-image: url(Images/@Model.Order_gifts.ElementAt(0).Image">
        <div class="container">
            <div class="row text-center">
                <div style="margin-top:150px" class="col-12">
                    <h4 style="    color: #ffffff;
                      font-size: 16px;
                      font-weight: 300;
                      letter-spacing: 2px;
                      margin-bottom: 10px;
                      text-transform: uppercase;">
                        @Model.Order_gifts.ElementAt(0).Description
                    </h4>
                    <h1 style="color: #ffffff;
                      font-size: 75px;
                      font-weight: 700;
                      line-height: 1em;
                      letter-spacing: 2px;
                      margin-bottom: 22px;
                      text-transform: uppercase;">
                        @Model.Order_gifts.ElementAt(0).Title
                    </h1>
                    <p>
                        @Model.Order_gifts.ElementAt(0).SubTitle
                    </p>
                    <button style="   background: #8b755a;color:white;font-weight:bold;font-size:2em;margin-top:20px !important;    border: 2px solid #8b755a;line-height: 24px;    text-align: center;   border-radius: 0px; font-size: 12px; letter-spacing: 2px; padding: 13px 43px; margin-top: 10px;cursor: pointer;text-transform:uppercase;">
                        order the gift
                    </button>
                </div>
            </div>
        </div>


    </section>

    <!-- SECTION Gift-Unique ends -->


</main>


